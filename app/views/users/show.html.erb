<table class="profile" summary="Profile information">
  <tr>
    <td class="main">

		<%= stylesheet_link_tag "event_calendar" %>

		<%= raw(event_calendar) %>
	   <% unless @user.moims.empty? %>
	        <table class="moims" summary="User moims">
	          <%= render @moims %>
	        </table>
	        <%= will_paginate @moims %>
	      <% end %>

    </td>
 	<%= render 'shared/user_info' %>
  </tr>
</table>



